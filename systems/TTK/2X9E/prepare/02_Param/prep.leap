source leaprc.protein.ff19SB
source leaprc.gaff
source leaprc.water.tip4pew
set default PBRadii mbondi2
loadoff lig.lib
loadamberparams lig.frcmod
WAT = T4E
HOH = T4E
loadAmberParams frcmod.ionsjc_tip4pew
loadAmberParams frcmod.tip4pew
mol = loadpdb 2X9E_his_fixed.pdb
solvateoct mol TIP4PEWBOX 8
addIons2 mol K+ 2
addIons2 mol Cl- 0
saveamberparm mol 2x9e.prmtop 2x9e.inpcrd
savepdb mol 2x9e_after_leap.pdb
check mol
charge mol
quit
